generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  subject   String
  email     String   @unique
  message   String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}
